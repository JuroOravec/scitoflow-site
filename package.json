{
  "name": "scitoflow-site",
  "private": true,
  "repository": "https://github.com/jurooravec/scitoflow-site",
  "author": "Juro Oravec <juraj.oravec.josefson@gmail.com>",
  "scripts": {
    "prebuild": "tsc --project tsconfig.server.json",
    "build": "run-s prebuild validate build:nocheck",
    "build:nocheck": "gridsome build",
    "dev": "run-s prebuild validate dev:nocheck",
    "dev:nocheck": "gridsome develop",
    "explore": "run-s prebuild validate explore:nocheck",
    "explore:nocheck": "gridsome explore",
    "validate": "node ./scripts/validateFrontmatter.js",
    "gql:gen": "node ./scripts/graphqlCodegen.js",
    "gql:gen:noserver": "graphql-codegen --config codegen.yml"
  },
  "_moduleAliases": {
    "@": "dist/gridsome"
  },
  "comments:scripts": {
    "validate": "Validate frontmatter of all Markdown files found by @gridsome/source-filesystem plugins",
    "gql:gen": "Start GraphQL dev server and run codegen against it",
    "gql:gen:noserver": "Same as gql:gen except it doesn't start the GraphQL server by itself. The server must be already running."
  },
  "comments:devDependencies": {
    "ts-loader": "DO NOT UPGRADE! v8.2 was failing to parse Vue files",
    "vuetify": "Dependencies: `deepmerge fibers sass sass-loader@7.3.1 vuetify-loader`, see https://gridsome.org/docs/assets-css/#vuetify",
    "sass": "Stay below 1.33 to avoid conflict with Vuetify, see https://github.com/telerik/kendo-themes/issues/2505"
  },
  "comments:overrides": {
    "graphql": "Force-bump graphql to v15 in gridsome@0.7.23 to enable interfaces to implement other interfaces."
  },
  "dependencies": {
    "@types/joi": "^17.2.3",
